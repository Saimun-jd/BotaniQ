{
  "expo": {
    "name": "BotaniQ",
    "slug": "BotaniQ",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/botaniq-logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "plugins": [
      [
        "expo-notifications",
        {
          "color": "#ffffff"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Please accept to add plant photos"
        }
      ],
      [
        "@react-native-google-signin/google-signin"
      ],
      [

        "expo-asset",
        {
          "assets": ["./assets/images/botaniq-logo.png", "./BotaniQ/BotaniQ/assets/images"]
        }
      ]
    ],
    "splash": {
      "image": "./assets/images/botaniq-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "android": {
        "permissions": [
          "CAMERA",
          "READ_EXTERNAL_STORAGE",
          "WRITE_EXTERNAL_STORAGE",
          "android.permission.RECEIVE_BOOT_COMPLETED",
          "CAMERA",
          "READ_EXTERNAL_STORAGE",
          "WRITE_EXTERNAL_STORAGE",
          "android.permission.RECEIVE_BOOT_COMPLETED"
        ]
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.anonymous.BotaniQ"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "91cf2e37-10d3-42be-ba19-83cbe368093e"
      }
    }
  }
}
